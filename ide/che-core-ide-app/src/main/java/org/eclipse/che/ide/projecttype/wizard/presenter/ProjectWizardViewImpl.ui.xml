<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>
        .rootPanel {
            position: relative;
        }

        .privacy label {
            padding-left: 17px;
        }

        .tab {
            left: 0px;
            top: 0px;
            right: 0px;
            bottom: 0px;
            position: absolute !important;
        }

        .labelPanel > div {
            margin-top: 4px;
        }
    </ui:style>

    <g:FlowPanel width="774px" height="470px" addStyleNames="{style.rootPanel}">
        <g:SimplePanel ui:field="wizardPanel" addStyleNames="{style.tab}"/>
    </g:FlowPanel>
</ui:UiBinder>
