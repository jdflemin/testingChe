<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:style>
        .panel {
            margin-top: 6px;
        }
    </ui:style>

    <g:ScrollPanel width="100%" height="100%">
        <g:FlowPanel addStyleNames="{style.panel}" ui:field="properties"/>
    </g:ScrollPanel>
</ui:UiBinder>